                <div class="dropdown-divider"></div>
                <div class="container py-5">
                    <div class="row">
                        <div class="col-12">
                            <div class="h5 pb-3 text-primary">Documentos&nbsp;</div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12-col-md-12 col-lg-6">
                            <div class="form-group" id="archivos-documentos">
                                @{
                                    var total = 0;
                                    if (Model.iniciativa_mit.ListaSustentos != null)
                                    {
                                        total = Model.iniciativa_mit.ListaSustentos.Count();
                                    }
                                }
                                <label class="font-weight-bold">Archivos adjuntos: <span id="total-documentos">@total</span> en total</label>
                                @{
                                    if (Model.iniciativa_mit.ListaSustentos != null)
                                    {
                                        var extension = "fa-file-word";

                                        foreach (var item in Model.iniciativa_mit.ListaSustentos)
                                        {
                                            if (item.ADJUNTO_BASE.Contains("pdf"))
                                            {
                                                extension = "fa-file-pdf";
                                            }
                                            else
                                            {
                                                if (item.ADJUNTO_BASE.Contains("jpeg") || item.ADJUNTO_BASE.Contains("png") || item.ADJUNTO_BASE.Contains("jpg") || item.ADJUNTO_BASE.Contains("gif"))
                                                {
                                                    extension = "fa-file-image";
                                                }
                                                else
                                                {
                                                    if (item.ADJUNTO_BASE.Contains("xls") || item.ADJUNTO_BASE.Contains("xlsx"))
                                                    {
                                                        extension = "fa-file-excel";
                                                    }
                                                    else
                                                    {
                                                        if (item.ADJUNTO_BASE.Contains("ppt") || item.ADJUNTO_BASE.Contains("pptx"))
                                                        {
                                                            extension = "fa-file-powerpoint";
                                                        }
                                                        else
                                                        {
                                                            if (item.ADJUNTO_BASE.Contains("doc") || item.ADJUNTO_BASE.Contains("docx"))
                                                            {
                                                                extension = "fa-file-word";
                                                            }
                                                            else
                                                            {
                                                                extension = "fa-file";
                                                            }
                                                        }
                                                    }
                                                }
                                            }

                                            <div class="input-group mb-3">
                                                <div class="input-group-append mr-3"><span class="input-group-text cursor-pointer" onclick="fn_verfilesutento(@item.ID_INICIATIVA_SUSTENTATORIO)"><i class="fas fa-download"></i></span></div>
                                                <div class="input-group-prepend"><span class="input-group-text"><i class="fas @extension"></i></span></div><span class="form-control-plaintext">@item.ADJUNTO_BASE</span>
                                                @*<div class="input-group-append"><span class="input-group-text cursor-pointer" onclick="fn_verfilesutento(@item.ID_INICIATIVA_SUSTENTATORIO)"><i class="fas fa-download"></i></span></div>*@
                                            </div>
                                        }
                                    }
                                }
                            </div>
                        </div>
                        <div class="col-sm-12-col-md-12 col-lg-6">&nbsp;</div>
                    </div>
                    <div class="dropdown-divider my-5"></div>
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-6">&nbsp;</div>
                        <div class="col-sm-12 col-md-12 col-lg-6 text-right">
                            <div class="row justify-content-end">
                                <div class="col-lg-6">&nbsp;</div>
                                <div class="col-lg-6"><a class="btn btn-dark px-5 w-100 text-center mb-3" href="#" onclick="fn_regresarListaIniciativa();" data-toggle="tooltip" data-placement="top" title="Has click aquí para retroceder al paso anterior"><i class="fas fa-hand-point-left pr-1"></i>Retroceder</a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

