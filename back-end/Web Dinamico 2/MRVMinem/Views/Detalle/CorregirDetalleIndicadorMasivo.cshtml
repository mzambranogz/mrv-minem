            error: function (myXhr, textStatus, errorThrown) {
                console.log(myXhr);
                console.log(textStatus);
                console.log(errorThrown);
            }
        };

        $("#formRegistrar").ajaxForm(options);
        $("#formRegistrar").submit();

    }

    document.getElementById('fledocumentos').addEventListener('change', handleFileSelect, false);
    document.getElementById('fledeclaracion').addEventListener('change', fn_CargarExcel, false);

    function limpiarAcumulado() {
        for (var j = 2010; j < 2031; j++) {
            $("#acum-" + j).html("0.00");
        }
    }

</script>
